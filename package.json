{
  "name": "video-chat-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "format": "js-beautify -r -f '**/*.ts' -f '**/*.tsx'",
    "prepare": "husky install",
    "test": "vitest"
