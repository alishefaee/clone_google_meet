    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.6",
    "@mui/material": "^6.1.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "js-beautify": "^1.15.1",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,md}": "js-beautify -r -f '**/*.ts' -f '**/*.tsx'"
